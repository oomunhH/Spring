<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<context:annotation-config></context:annotation-config>
	
	<bean id="xx" class="com.dto.Cat">
		<property name="catAge" value="100"></property>
		<property name="catName" value="야옹이2"></property>
	</bean>
	<bean id="yy" class="com.dto.Cat" primary="true">
		<property name="catAge" value="1000"></property>
		<property name="catName" value="야옹이3"></property>
	</bean>
	
	<bean id="person1" class="com.dto.Person" autowire="byType">
		<property name="username" value="홍길동"></property>
	
	</bean>
	
</beans>
